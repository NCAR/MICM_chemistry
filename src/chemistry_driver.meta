  units = s
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[options_filepath]
  standard_name = options_filepath
  long_name = Options file path
  units = none
  dimensions = ()
  type = character
  kind = len=*
  intent = in
[print_log_message]
  standard_name = print_log_message
